spring:
  datasource:
    url: jdbc:mysql://{dbURL}?serverTimezone=UTC&characterEncoding=UTF-8
    username: {id}
    password: {pw}
    driver-class-name: com.mysql.cj.jdbc.Driver

  jpa:
    show-sql: true
    open-in-view: false
    hibernate:
      ddl-auto: update
    properties:
      hibernate:
        format_sql: true
        default_batch_fetch_size: 100
        dialect: org.hibernate.dialect.MySQL8Dialect

  data:
    redis:
      host: {hostName}
      port: {port}

  jwt:
    access-secret: {accessSecret}
    refresh-secret: {refreshSecret}
    access-expiration: {accessExpiration}
    refresh-expiration: {refreshExpiration}