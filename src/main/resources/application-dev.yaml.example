spring:
  datasource:
    url: jdbc:mysql://{dbURL}?serverTimezone=UTC&characterEncoding=UTF-8
    username: {id}
    password: {pw}
    driver-class-name: com.mysql.cj.jdbc.Driver

  jpa:
    hibernate:
      ddl-auto: validate
    properties:
      hibernate:
        format_sql: true
        

          jpa:
            open-in-view: false
            defer-datasource-initialization: true
            hibernate.ddl-auto: update
            show-sql: true
            properties:
              hibernate.format_sql: true
              hibernate.default_batch_fetch_size: 100
              hibernate:
                dialect: org.hibernate.dialect.MySQLDialect